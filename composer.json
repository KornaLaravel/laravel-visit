{
    "name": "spatie/laravel-visit",
    "description": "Quickly visit any route of your Laravel app",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-visit"
    ],
    "homepage": "https://github.com/spatie/laravel-visit",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0|^12.0",
        "nunomaduro/termwind": "^2.0",
        "soundasleep/html2text": "^2.0",
        "spatie/laravel-package-tools": "^1.11.1",
        "symfony/css-selector": "^6.0|^7.2",
        "symfony/dom-crawler": "^6.0|^7.2",
        "symfony/stopwatch": "^6.0|^7.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^v8.1.1",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.34|^3.7",
        "pestphp/pest-plugin-laravel": "^2.3|^3.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "phpstan/phpstan-phpunit": "^1.0|^2.0",
        "phpunit/phpunit": "^10.5.16|^11.5.3",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Visit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Visit\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Visit\\VisitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
